{
  "rewrites": [
    { "source": "/", "destination": "pages/votacao.html" }
  ],
  "redirects": [
    {
      "source": "/pages",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/pages/",
      "destination": "/",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
